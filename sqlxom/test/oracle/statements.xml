<?xml version="1.0" encoding="utf-8"?>
<oracle:block
  xmlns:oracle="http://www.bphx.com/basic-sql/2008-12-11/oracle"
  xmlns:sql="http://www.bphx.com/basic-sql/2008-12-11">

  <sql:select>
    <sql:with>
      <sql:source alias="T">
        <sql:column name="A"/>
        <sql:column name="B"/>
        <sql:column name="C"/>
      </sql:source>

      <sql:select>
        <sql:column>
          <sql:number value="1"/>
        </sql:column>
        <sql:column>
          <sql:number value="2"/>
        </sql:column>
        <sql:column>
          <sql:number value="3"/>
        </sql:column>

        <sql:from>
          <sql:table name="D"/>
        </sql:from>
        <sql:where>
          <sql:eq>
            <sql:field name="E"/>
            <sql:number value="7"/>
          </sql:eq>
        </sql:where>
      </sql:select>
    </sql:with>

    <sql:column wildcard="true"/>

    <sql:from>
      <sql:table name="T"/>
    </sql:from>
  </sql:select>
</oracle:block>